#!/usr/bin/env python3
# This file is placed in the Public Domain.


"create env"


import os


def popen(txt):
    for line in os.popen(txt).readlines():
        print(line.rstrip())



popen("rm -fR ~/.cache/pip")
popen("sbin/clean")
popen("sbin/build")
popen("python3 -mvirtualenv --no-download env")
popen("env/bin/pip3 install objectprogramming -f dist --force-reinstall --no-deps")
